{
    "name": "zara-pullandbear-scraper",
    "version": "4.0.0",
    "description": "Production-ready Zara & Pull&Bear product scraper with 16,071+ products support",
    "main": "src/main.ts",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "scrape": "npx tsx src/main.ts",
        "build": "tsc",
        "start": "node dist/main.js",
        "dev": "npx tsx src/main.ts",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf output/* dist/*",
        "help": "echo 'Available scripts: scrape, build, start, dev, type-check, clean'"
    },
    "keywords": [
        "scraper",
        "ecommerce",
        "zara",
        "pullandbear",
        "typescript"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^20.x",
        "typescript": "^5.x"
    },
    "dependencies": {
        "@prisma/client": "^6.13.0",
        "prisma": "^6.13.0"
    }
}