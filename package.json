{
    "name": "zara-pullandbear-scraper",
    "version": "4.0.0",
    "description": "Production-ready Zara & Pull&Bear product scraper with 16,071+ products support",
    "main": "scripts/production-scraper.ts",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "scrape:categories": "npx ts-node scripts/category-scraper-v2.ts",
        "scrape:test": "npx ts-node scripts/test-minimal-api.ts",
        "scrape:production": "npx ts-node scripts/production-scraper.ts",
        "scrape:all": "npm run scrape:categories && npm run scrape:test && npm run scrape:production",
        "build": "tsc --noEmit",
        "type-check": "tsc --noEmit",
        "dev": "npm run scrape:test",
        "start": "npm run scrape:production",
        "clean": "rm -rf output/*.json",
        "docs": "echo 'Documentation available in docs/ folder'",
        "help": "echo 'Available scripts: scrape:categories, scrape:test, scrape:production, scrape:all'"
    },
    "keywords": [
        "scraper",
        "ecommerce",
        "zara",
        "pullandbear",
        "typescript"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^20.x",
        "typescript": "^5.x"
    },
    "dependencies": {
        "@prisma/client": "^6.13.0",
        "prisma": "^6.13.0"
    }
}