{
    "name": "zara-pullandbear-scraper",
    "version": "4.2.0",
    "description": "Production-ready Zara & Pull&Bear scraper with Prisma database integration - 29,755+ products support",
    "main": "src/main.ts",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "scrape": "npx tsx src/main.ts",
        "scrape:modular": "npx tsx src/main-modular.ts",
        "scrape:production": "npx tsx src/main-production.ts",
        "scrape:final": "npx tsx src/main-final.ts",
        "db:push": "npx prisma db push",
        "db:generate": "npx prisma generate",
        "db:studio": "npx prisma studio",
        "db:reset": "npx prisma db push --force-reset",
        "build": "tsc",
        "start": "node dist/main.js",
        "dev": "npx tsx src/main.ts",
        "dev:modular": "npx tsx src/main-modular.ts",
        "dev:production": "npx tsx src/main-production.ts",
        "dev:final": "npx tsx src/main-final.ts",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf output/* dist/*",
        "test:system": "npx tsx tests/system-test.ts",
        "logs:view": "tail -f logs/combined.log",
        "logs:errors": "tail -f logs/error.log",
        "help": "echo 'Available scripts: scrape, scrape:modular, scrape:production, build, start, dev, dev:modular, dev:production, type-check, clean, test:system, logs:view, logs:errors, db:push, db:generate, db:studio, db:reset'"
    },
    "keywords": [
        "scraper",
        "ecommerce",
        "zara",
        "pullandbear",
        "typescript"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^20.19.9",
        "typescript": "^5.x"
    },
    "dependencies": {
        "@prisma/client": "^6.13.0",
        "dotenv": "^17.2.1",
        "mysql2": "^3.14.3",
        "prisma": "^6.13.0",
        "winston": "^3.17.0",
        "zod": "^4.0.15"
    }
}